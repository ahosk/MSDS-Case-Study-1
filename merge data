library(tidyr)
library(tidyverse)
library(magrittr)
library(dplyr)

#reading in data
beers = read_csv(file.choose())
breweries = read_csv(file.choose())

head(beers)
head(breweries)
#renaming columns to match
names(beers)[1] = 'Beer_Name'
names(beers)[5] = 'Brew_ID'

#merging data sets
bb = left_join(breweries,beers,by = NULL )
head(bb)
